@article{jaeger2017,
  title = {Finding Water Scarcity amid Abundance Using Human\textendash Natural System Models},
  author = {Jaeger, William K. and Amos, Adell and Bigelow, Daniel P. and Chang, Heejun and Conklin, David R. and Haggerty, Roy and Langpap, Christian and Moore, Kathleen and Mote, Philip W. and Nolin, Anne W. and Plantinga, Andrew J. and Schwartz, Cynthia L. and Tullos, Desiree and Turner, David P.},
  year = {2017},
  month = nov,
  journal = {Proceedings of the National Academy of Sciences},
  volume = {114},
  number = {45},
  pages = {11884--11889},
  issn = {0027-8424, 1091-6490},
  doi = {10.1073/pnas.1706847114},
  abstract = {Significance             Climate change will heighten the need to anticipate water shortages worldwide. The task is daunting due to water's variability, spatial-temporal movement, feedbacks, and other system complexities. A high-resolution coupled human\textendash natural system model identifies how both climate change and socioeconomic drivers will alter water scarcity in future decades. The results illuminate how water scarcity varies greatly across small distances and brief time periods, even in basins where water may be relatively abundant overall. These findings, and other unexpected results that may seem counterintuitive, underscore the potential value of such models for policy.           ,              Water scarcity afflicts societies worldwide. Anticipating water shortages is vital because of water's indispensable role in social-ecological systems. But the challenge is daunting due to heterogeneity, feedbacks, and water's spatial-temporal sequencing throughout such systems. Regional system models with sufficient detail can help address this challenge. In our study, a detailed coupled human\textendash natural system model of one such region identifies how climate change and socioeconomic growth will alter the availability and use of water in coming decades. Results demonstrate how water scarcity varies greatly across small distances and brief time periods, even in basins where water may be relatively abundant overall. Some of these results were unexpected and may appear counterintuitive to some observers. Key determinants of water scarcity are found to be the cost of transporting and storing water, society's institutions that circumscribe human choices, and the opportunity cost of water when alternative uses compete.},
  langid = {english},
  keywords = {ğŸŒŸæ˜Ÿæ ‡,ğŸ‘€æµè§ˆ,ğŸ“šæ¨è,ç‹è€å¸ˆ},
  annotation = {publication: \{"uuid":1414677,"tempID":0,"paperName":"PROCEEDINGS OF THE NATIONAL ACADEMY OF SCIENCES","swjtu":"A++"\}},
  file = {/Users/songshgeo/Zotero/storage/HDUBEK59/PNAS-Supporting information.pdf;/Users/songshgeo/Zotero/storage/PADUI2JM/Jaeger ç­‰ã€‚ - 2017 - Finding water scarcity amid abundance using humanâ€“.pdf}
}

@article{vaighan2017,
