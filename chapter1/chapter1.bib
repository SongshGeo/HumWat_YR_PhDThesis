@article{ahlstrom2021,
  title = {An {{Earth}} System Law Perspective on Governing Social-Hydrological Systems in the {{Anthropocene}}},
  author = {Ahlstr{\"o}m, Hanna and Hileman, Jacob and {Wang-Erlandsson}, Lan and Garc{\'i}a, Mar{\'i}a Mancilla and Moore, Michele-Lee and Jonas, Krisztina and Pranindita, Agnes and Kuiper, Jan J. and Fetzer, Ingo and Jaramillo, Fernando and Svedin, Uno},
