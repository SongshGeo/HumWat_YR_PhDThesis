@article{mekonnen2011,
title = {The Green, Blue and Grey Water Footprint of Crops and Derived Crop Products},
author = {Mekonnen, M. M. and Hoekstra, A. Y.},
year = {2011},
month = may,
journal = {Hydrology and Earth System Sciences},
volume = {15},
number = {5},
pages = {1577--1600},
issn = {1607-7938},
doi = {10.5194/hess-15-1577-2011},
abstract = {This study quantifies the green, blue and grey water footprint of global crop production in a spatially-explicit way for the period 1996\textendash 2005. The assessment improves upon earlier research by taking a high-resolution approach, estimating the water footprint of 126 crops at a 5 by 5 arc minute grid. We have used a grid-based dynamic water balance model to calculate crop water use over time, with a time step of one day. The model takes into account the daily soil water balance and climatic conditions for each grid cell. In addition, the water pollution associated with the use of nitrogen fertilizer in crop production is estimated for each grid cell. The crop evapotranspiration of additional 20 minor crops is calculated with the CROPWAT model. In addition, we have calculated the water footprint of more than two hundred derived crop products, including various flours, beverages, fibres and biofuels. We have used the water footprint assessment framework as in the guideline of the Water Footprint Network.},
langid = {english},
annotation = {SC: 0001846 sciif: 6.62 sci: Q1 publication: \{"uuid":1430104,"tempID":0,"paperName":"HYDROLOGY AND EARTH SYSTEM SCIENCES","xju":"ä¸€åŒº","cug":"åœ°å­¦T1","sciif":"6.62","sci":"Q1","sciBase":"åœ°å­¦1åŒº","sciUp":"åœ°çƒç§‘å­¦2åŒº","sciUpSmall":"åœ°çƒç§‘å­¦ï¼šç»¼åˆ2åŒº/æ°´èµ„æº2åŒº","eii":"EI","scu":"åœ°çƒç§‘å­¦B","jci":"1.49"\}},
