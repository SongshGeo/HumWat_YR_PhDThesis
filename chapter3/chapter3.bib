@article{zhang1994,
  title = {Evidence for the Existence of the Medieval Warm Period in {{China}}},
  author = {Zhang, De'Er},
  year = {1994},
  month = mar,
  journal = {Climatic Change},
  volume = {26},
  number = {2},
  pages = {289--297},
  issn = {1573-1480},
  doi = {10.1007/BF01092419},
  abstract = {The collected documentary records of the cultivation of citrus trees andBoehmeria nivea (a perennial herb) have been used to produce distribution maps of these plants for the eighth, twelfth and thirteenth centuries A.D. The northern boundary of citrus andBoehmeria nivea cultivation in the thirteenth century lay to the north of the modern distribution. During the last 1000 years, the thirteenth-century boundary was the northernmost. This indicates that this was the warmest time in that period. On the basis of knowledge of the climatic conditions required for planting these species, it can be estimated that the annual mean temperature in south Henan Province in the thirteenth century was 0.9\textendash 1.0\textdegree C higher than at present. A new set of data for the latest snowfall date in Hangzhou from A.D. 1131 to 1264 indicates that this cannot be considered a cold period, as previously believed.},
  langid = {english},
  keywords = {\#thesis,â›“ï¸å…³è”,ğŸ”æ£€ç´¢,climate change,historical geography},
  file = {/Users/songshgeo/Zotero/storage/643SSF26/Zhang_1994_Evidence for the existence of the medieval warm period in China.pdf}
}

@article{ge2008,
  title = {Meiyu in the Middle and Lower Reaches of the {{Yangtze River}} since 1736},
  author = {Ge, QuanSheng and Guo, XiFeng and Zheng, JingYun and Hao, ZhiXin},
  year = {2008},
  month = jan,
  journal = {Chinese Science Bulletin},
  volume = {53},
  number = {1},
  pages = {107--114},
  issn = {1861-9541},
  doi = {10.1007/s11434-007-0440-5},
  abstract = {``Yu Xue Fen Cun'' records during the Qing Dynasty are used to identify the starting and ending dates of Meiyu at the period of 1736\textendash 1911. These results, along with the instrumental meteorological records, are used to reconstruct the series of length and precipitation of Meiyu during 1736\textendash 2000 over the middle and lower reaches of the Yangtze River. The characteristics of Meiyu are analyzed since 1736. Moreover, the strength of East Asian Summer Monsoon and locations of rainband are discussed, based on the relationship between the length of Meiyu and the Index of East Asian Summer Monsoon. It is found that the starting and ending dates and the length of Meiyu have significant interannual and interdecadal variations. Apart from 7\textendash 8 years, 20\textendash 30 years and 40 years cycles for the lengths of Meiyu, the centennial oscillation is also presented. The length of Meiyu, monsoon rainband movement over eastern China, and the strength of East Asian Summer Monsoon (EASM) have a very good correlation, which can be expressed in the following: during the periods of 1736\textendash 1770, 1821\textendash 1870 and 1921\textendash 1970, the EASM was stronger, and the monsoon rainband was located in North China and South China easily, corresponding to the decreased length of Meiyu. Whereas during the periods of 1771\textendash 1820, 1871\textendash 1920 and 1971\textendash 2000, the EASM was weaker and monsoon rainband usually stopped at the middle and lower reaches of the Yangtze River, corresponding to the increased length of Meiyu.},
  langid = {english},
  keywords = {to-read},
  file = {/Users/songshgeo/Zotero/storage/Q3GYSZ8L/Ge et al_2008_Meiyu in the middle and lower reaches of the Yangtze River since 1736.pdf}
}

